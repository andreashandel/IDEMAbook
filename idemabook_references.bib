  pmid = {22028909}
}

@article{leontides94,
  title = {Herd Risk Factors for Serological Evidence of {{Aujeszky}}'s Disease Virus Infection of Breeding Sows in Northern {{Germany}} (1990-1991).},
  author = {Leontides, L and Ewald, C and Willeberg, P},
  year = {1994},
